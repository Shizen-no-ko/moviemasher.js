{
  "entryPoints": ["src/index.ts"],
  "name": "moviemasher.js",
  "includeVersion": true,
  "exclude": [
    "**/test/**/*.*",
    "**/index.ts"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "categoryOrder": ["Interfaces", "Enumerations", "Classes", "*"],
  "categorizeByGroup": true,
  "hideGenerator": true,
  "theme": "./node_modules/typedoc-neo-theme/bin/default",
  "outline": [
    {
      "Core": {
        "MovieMasher": "classes/moviemasher",
        "Definition": "interfaces/definition",
        "Instance": "interfaces/instance"

      },

      "Media": {
        "Audio": "interfaces/audio",
        "Effect": "interfaces/effect",
        "Filter": "interfaces/filter",
        "Font": "interfaces/font",
        "Image": "interfaces/image",
        "Mash": "interfaces/mash",
        "Masher": "interfaces/masher",
        "Merger": "interfaces/merger",
        "Scaler": "interfaces/scaler",
        "Theme": "interfaces/theme",
        "Transition": "interfaces/transition",
        "Video": "interfaces/video"
      }
    }
  ],
  "search": [{
    "name": "interfaces/*",
    "subtitle": "Interfaces"
  }],
  "source": [{
    "path": "https://github.com/moviemasher/moviemasher.js/tree/master/",
    "line": "L"
  }]
}
