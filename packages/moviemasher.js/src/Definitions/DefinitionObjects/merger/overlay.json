{
  "label": "Overlay",
  "id": "com.moviemasher.merger.overlay",
  "type": "merger",
  "properties": [
    { "name": "left", "defaultValue": 0.5, "min": 0, "max": 1.0, "step": 0.001 },
    { "name": "top", "defaultValue": 0.5, "min": 0, "max": 1.0, "step": 0.001 }
  ],
  "filters": [
    {
      "id": "com.moviemasher.filter.overlay",
      "parameters": [
        {
          "name": "x",
          "value": "((main_w + overlay_w) * left) - overlay_w"
        },
        {
          "name": "y",
          "value": "((main_h + overlay_h) * top) - overlay_h"
        }
      ]
    }
  ]
}
