{
  "name": "@moviemasher/moviemasher.js",
  "description": "JavaScript ES6 module for realtime, browser-based video and audio editing",
  "homepage": "https://github.com/moviemasher/moviemasher.js#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "source": "src/index.js",
  "version": "4.0.26",
  "keywords": [
    "video editor",
    "audio editor"
  ],
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/moviemasher/moviemasher.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moviemasher/moviemasher.js.git"
  },
  "scripts": {
    "build": "run-s test clean-rollup",
    "build-watch": "rollup --watch --config build/rollup.config.js",
    "test": "run-s lint jest-verbose",
    "test-watch": "jest --watch --config test/jest.config.js",
    "lint": "eslint 'src/**/*.ts'",
    "serve": "run-s clean serve_html serve_local",
    "help": "run-s typescript-help lint-help build-help test-help",
    "clean-rollup": "run-s clean rollup",
    "rollup": "rollup --config build/rollup.config.js",
    "serve_html": "node lib/wrap-html.js",
    "serve_local": "run-p serve_js serve_livereload serve_httpserver",
    "serve_js": "NODE_ENV=development rollup -c -w",
    "serve_livereload": "livereload dist",
    "serve_httpserver": "http-server ./dist -o -d false",
    "jest-verbose": "jest --verbose --coverage --config test/jest.config.js",
    "jest": "jest --config test/jest.config.js",
    "clean": "rimraf dist",
    "typescript-help": "tsc -h",
    "build-help": "rollup --help",
    "test-help": "jest -h",
    "lint-help": "eslint -h"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@wessberg/rollup-plugin-ts": "^1.3.14",
    "canvas": "^2.7.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^4.4.1",
    "livereload": "^0.9.3",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-flat-dts": "^1.2.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-smart-asset": "^2.1.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "reactjs-moviemasher"
  ],
  "types": "dist/moviemasher.d.ts"
}
