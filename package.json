{
  "name": "@moviemasher/moviemasher.js",
  "description": "JavaScript ES6 module for fast, browser-based video and audio editing",
  "homepage": "https://moviemasher.com",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "source": "src/index.ts",
  "version": "5.0.0",
  "keywords": [
    "video editor",
    "audio editor",
    "esm",
    "cjs",
    "umd",
    "module",
    "canvas",
    "composite"
  ],
  "license": "MPL-2.0",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "bugs": {
    "url": "https://github.com/moviemasher/moviemasher.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moviemasher/moviemasher.js.git"
  },
  "scripts": {
    "build-react": "run-s build-react-package build-react-test",
    "build-react-package": "npm run build -w @moviemasher/react-moviemasher",
    "build-react-test": "npm run build -w react-moviemasher",
    "build-cjs": "rollup --config dev/rollup/cjs.config.js",
    "build-docs": "node ./dev/documentation",
    "build-esm": "rollup --config dev/rollup/esm.config.js",
    "build-help": "rollup --help",
    "build-only": "rollup --config",
    "build-umd": "rollup --config dev/rollup/umd.config.js",
    "build": "run-s lint-verbose test-verbose clean build-esm build-umd build-cjs",
    "clean": "rimraf dist",
    "develop": "rollup --watch --config dev/rollup/esm.config.js",
    "help": "run-s lint-help test-help transpile-help build-help",
    "lint-help": "eslint -h",
    "lint-verbose": "eslint --quiet --no-ignore src/**/*.[jt]s",
    "lint": "eslint src/**/*.ts",
    "md-help": "markdown --help",
    "start": "docker-compose --file dev/docker/compose.yml up -d",
    "logs": "docker-compose --file dev/docker/compose.yml logs -f",
    "stop": "docker-compose --file dev/docker/compose.yml down -v",
    "test-help": "jest -h",
    "test-verbose": "jest --verbose --coverage --config dev/test/jest.config.js",
    "test-watch": "jest --verbose  --watch --config dev/test/jest.config.js",
    "test": "jest --config dev/test/jest.config.js",
    "transpile-help": "tsc -h"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/css-font-loading-module": "^0.0.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.23",
    "@types/jest-image-snapshot": "^4.3.0",
    "@types/node": "^15.12.4",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "canvas": "^2.8.0",
    "eslint": "^7.29.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "fluent-ffmpeg-multistream": "^1.0.0",
    "jest": "^27.0.5",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^4.5.1",
    "markdown-magic": "^2.5.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.0",
    "serve": "^12.0.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.0",
    "typedoc": "^0.20.37",
    "typescript": "^4.2.4",
    "wrtc": "^0.4.7"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "**/workspaces/*",
    "./packages/*"
  ]
}
