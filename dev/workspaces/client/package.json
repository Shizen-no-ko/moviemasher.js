{
  "name": "client",
  "version": "5.0.0",
  "description": "Client",
  "author": {
    "name": "Movie Masher",
    "email": "support@moviemasher.com"
  },
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "scripts": {
    "build-rollup": "rollup --config",
    "build": "run-s clean build-rollup link-assets copy-html copy-icon",
    "clean": "rimraf dist",
    "copy-html": "cp ../../index.html dist",
    "copy-icon": "cp ../../assets/graphics/favicon.ico dist",
    "develop": "rollup --config --watch",
    "link-assets": "ln -s $(cd ../../assets ; pwd) dist",
    "serve": "serve --debug --symlinks --cors --no-port-switching --no-clipboard --listen 8573 dist"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@moviemasher/react-moviemasher": "^5.0.0",
    "@moviemasher/moviemasher.js": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remixicon-react": "^1.0.0"
  }
}
